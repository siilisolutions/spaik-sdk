{
    "name": "spaik-sdk-react",
    "version": "0.1.2",
    "description": "React library for Spaik SDK - state management and streaming for AI agent frontends",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "author": {
        "name": "Siili Solutions Oyj",
        "email": "info@siili.com",
        "url": "https://siili.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/siilisolutions/spaik-sdk.git",
        "directory": "packages/agent-sdk-hooks"
    },
    "homepage": "https://github.com/siilisolutions/spaik-sdk#readme",
    "bugs": {
        "url": "https://github.com/siilisolutions/spaik-sdk/issues"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "bun run tsc",
        "dev": "bun run tsc --watch",
        "lint": "bun run eslint src --ext .ts,.tsx",
        "lint:fix": "bun run eslint src --ext .ts,.tsx --fix",
        "type-check": "bun run tsc --noEmit"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "dependencies": {
        "axios": "^1.13.2",
        "zod": "^3.22.4",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "typescript": "^5.3.3"
    },
    "keywords": [
        "react",
        "hooks",
        "agent",
        "ai",
        "llm",
        "streaming",
        "state-management",
        "zustand",
        "zod",
        "spaik"
