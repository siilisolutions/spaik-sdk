{
    "name": "agent-sdk-monorepo",
    "private": true,
    "workspaces": [
        "packages/*",
        "examples/frontend"
    ],
    "scripts": {
        "build:hooks": "cd packages/agent-sdk-hooks && yarn build",
        "dev:frontend": "cd examples/frontend && yarn dev",
        "dev:backend": "cd examples/backend && uv run uvicorn main:app --reload",
        "install:all": "yarn install && cd packages/agent-sdk-hooks && yarn install && cd ../../examples/frontend && yarn install"
    },
    "devDependencies": {
        "typescript": "^5.3.3"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
